<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?> <!-- i18n: added to work around 2.1 bug -->
<plugin>

     <extension
         point="org.eclipse.ui.startup">
   </extension>
   <extension
         id="com.genuitec.eclipse.easie.jboss6.preferencepageextension"
         name="%prefPageExtensionName"
         point="org.eclipse.ui.preferencePages">
      <page
            name="%menu.label"
            category="com.genuitec.eclipse.easie.jboss.preferencepage"
            class="com.genuitec.eclipse.easie.jboss6.preference.JBoss6LauncherPreferencePage"
            id="com.genuitec.eclipse.easie.jboss6.preferencepage">
            <keywordReference id="com.genuitec.eclipse.easie.core.jboss.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.server.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.applicationserver.keyword"/>
      </page>
      <page
            name="%menu.jdk.label"
            category="com.genuitec.eclipse.easie.jboss6.preferencepage"
            class="com.genuitec.eclipse.easie.jboss6.preference.JBoss6LauncherJVMPreferencePage"
            id="com.genuitec.eclipse.easie.jboss6.jdkpage">
            <keywordReference id="com.genuitec.eclipse.easie.core.jboss.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.server.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.applicationserver.keyword"/>
      </page>
      <page
            name="%menu.path.label"
            category="com.genuitec.eclipse.easie.jboss6.preferencepage"
            class="com.genuitec.eclipse.easie.jboss6.preference.JBoss6LauncherClasspathPreferencePage"
            id="com.genuitec.eclipse.easie.jboss6.classpathpage">
            <keywordReference id="com.genuitec.eclipse.easie.core.jboss.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.server.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.applicationserver.keyword"/>
      </page>
      <page
            name="%menu.launch.label"
            category="com.genuitec.eclipse.easie.jboss6.preferencepage"
            class="com.genuitec.eclipse.easie.jboss6.preference.JBoss6LauncherModesPreferencePage"
            id="com.genuitec.eclipse.easie.jboss6.launchpage">
            <keywordReference id="com.genuitec.eclipse.easie.core.jboss.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.server.keyword"/>
            <keywordReference id="com.genuitec.eclipse.easie.core.applicationserver.keyword"/>
      </page>
   </extension>

</plugin>
