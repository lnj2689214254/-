	xmlns:md="http://org.eclipse.jst.jsf.common.metadata/metadata.ecore"
	xmlns:pi="http://org.eclipse.jsf.pagedesigner/paletteInfos.ecore"
	id="test"
	type="tagFile">

	<trait id="paletteInfos">
		<value xsi:type="pi:PaletteInfos">
			<item id="test:foobar"/>
		</value>
	</trait>
	
	<trait id="images-base-path">
		<value>icons/palette/test</value>
	</trait>
	
	<trait id="display-label">
		<value>Test</value>
	</trait>
	
	<entity id="test:foobar">
		<trait id="display-label">
			<value>%test.foobar.display-label</value>
		</trait>
		<trait id="description">
			<value>%test.foobar.description</value>
		</trait>
		<trait id="small-icon">
			<value>small/test.gif</value>
		</trait>
		<trait id="large-icon">
			<value>large/test.gif</value>
		</trait>
	</entity>
	
</md:metadatamodel>
	